<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../shared/components/headConfig.ejs') %>
    <link rel="stylesheet" href="../dist/authentication.css" />
  </head>
  <body>
    <!-- Start your project here-->
    <main>
      <div class="container-xxl py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container text-center py-5">
          <div class="row justify-content-center">
            <div class="col-lg-6">
              <img
                class="authenticate_img mb-3"
                src="../shared/img/authenticate.svg"
                alt=""
              />
              <h1 class="mb-4">Two-Factor Login</h1>
              <div class="form-outline mb-4">
                <input id="code" class="form-control form-control-lg" />
                <label class="form-label" for="code">Code</label>
              </div>
              <a
                id="submitButton"
                class="btn btn-primary rounded-pill py-3 px-5"
                >Submit</a
              >
            </div>
          </div>
        </div>
      </div>
    </main>
    <!-- End your project here-->
    <!-- MDB -->
    <script src="../dist/twoFactorLogin.js" type="text/javascript"></script>
    <script src="../shared/js/mdb.min.js" type="text/javascript"></script>
  </body>
</html>
