<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../shared/components/headConfig.ejs') %>
    <link rel="stylesheet" href="../dist/authentication.css" />
  </head>
  <body>
    <!-- Start your project here-->
    <main>
      <div class="container-xxl py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container text-center py-5">
          <div class="row justify-content-center">
            <div class="col-lg-6">
              <img
                class="Email_img mb-3"
                src="../shared/img/mail1.svg"
                alt=""
              />
              <h1 class="mb-4">Forget Password?</h1>
              <div class="form-outline mb-4">
                <input
                  type="email"
                  id="email"
                  class="form-control form-control-lg"
                />
                <label class="form-label" for="email">Enter your Email</label>
              </div>
              <a
                class="btn btn-primary rounded-pill py-3 px-5"
                id="forgetPasswordButton"
                >Send Code</a
              >
            </div>
          </div>
        </div>
      </div>
    </main>
    <!-- End your project here-->
    <!-- MDB -->
    <!-- JavaScript Libraries -->
    <script src="../dist/forgetPassword.js" type="text/javascript"></script>
    <script src="../shared/js/mdb.min.js" type="text/javascript"></script>
  </body>
</html>
